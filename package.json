{
  "name": "serverless-service",
  "version": "1.0.0",
  "main": "service.js",
  "scripts": {
    "deploy:service": "serverless deploy function -f service --aws-profile testProfile",
    "lint": "tslint --project tsconfig.json **/*.ts",
    "local:service": "serverless invoke local --function service --path events/service.json",
    "remote:service": "serverless invoke --function service",
    "tail:service:dev": "serverless logs --function service --tail --stage dev --aws-profile testProfile",
    "test": "jest --watchAll --colors --coverage",
    "test:ci": "jest --ci --colors --coverage --passWithNoTests"
  },
  "dependencies": {
    "uuidv4": "^6.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^24.0.11",
    "@types/node": "^8.0.57",
    "aws-sdk": "^2.427.0",
    "jest": "^24.5.0",
    "serverless": "^1.83.2",
    "serverless-ini-env": "^1.0.13",
    "serverless-plugin-existing-s3": "^2.3.3",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-webpack": "^5.1.1",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.2.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^2.9.2",
    "webpack": "^4.5.0"
  },
  "author": "Bonganjalo Hadebe",
  "license": "ESC"
}
